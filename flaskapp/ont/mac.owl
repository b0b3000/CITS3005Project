<owl:NamedIndividual rdf:about="#Replacing~Upgrading_Mac_Pro_Processors_(Early_2008)/step2">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#https://d3nevzfk7ii3be.cloudfront.net/igi/OLWWDAFfYqKnji1K.standard"/>
  <step_number rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_number>
  <step_description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The hard drives must meet the following specifications: Type: Serial Attached SCSI (SAS) or Serial ATA (SATA) 3 Gb/s Width: 3.9 inches (102 mm) Depth: 5.7 inches (147 mm) Height: 1.0 inch</step_description>
